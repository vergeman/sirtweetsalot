<!DOCTYPE html>
<html>

    <head>
        <title><%= title %></title>
        <%= stylesheet_link_tag    'application', media: 'all'%>
        <%= yield :head %>
        <%= csrf_meta_tags %>
        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
         <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
         <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
         <![endif]-->
        <meta name="google-site-verification" content="rcDYVA5YTQUn0S8RLO4WHAy8egKZX_19bZwuEi2EVlA" />
    </head>

    <body class="app">

        <%= render partial: "/layouts/navbar" unless current_user.nil? %>

        <div class="container-fluid">

            <div class="row">
                <div class="subheader col-sm-12 col-md-12">
                    <%= yield :header %>
                </div>

                <div class="col-sm-12 col-md-12 main">
                    <%= yield %>
                </div>

            </div> <!-- end row -->
        </div> <!-- end container -->


    </body>

    <%= javascript_include_tag 'application' %>
    <%= yield :js %>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-61110929-1', 'auto');
      ga('send', 'pageview');

    </script>

</html>
